@5901 = ~Consider it done.~ [zgnxani]    ///  cd_action1 #i
@5902 = ~There.~ [zgnxanj]    ///  cd_action2 #j
@5903 = ~Yes.~ [zgnxank]    ///  cd_action3 #k
@5904 = ~Well, look at the big brain on you!~ [zgnxans]    ///  cd_action4 #s
@5905 = ~This way!~ [zgnxant]    ///  cd_action5 #t
@5906 = ~It is not so difficult.~ [zgnxanu]    ///  cd_action6 #u
@5907 = ~Won't this be fun...~ [zgnxanv]    ///  cd_action7 #v
@5916 = ~Let's make this quick, shall we?~ [zgnxana]    ///  cd_battlecry1 #a
@5917 = ~Taste true power!~ [zgnxan8]    ///  cd_battlecry2 #8
@5918 = ~You'll not live long, fool!~ [zgnxan9]    ///  cd_battlecry3 #9
@5919 = ~You cannot win!~ [zgnxang_]   ///  cd_battlecry4 #g_
@5920 = ~Is that your face or has your neck thrown up?~ [zgnxanh_]   ///  cd_battlecry5 #h_
@5921 = ~At this rate we'll get quite far, won't we?~ [zgnxand]    ///  cd_bored #d
@5922 = ~So, have you come to your senses?~ [zgnxani_]   ///  cd_bored2 #i_
@5924 = ~These halflings grow more irritating by the minute.~ [zgnxano]    ///  cd_city #o
@5925 = ~Mm?~ [zgnxanf]    ///  cd_common1 #f
@5926 = ~You have something to say?~ [zgnxang]    ///  cd_common2 #g
@5927 = ~A-hem!~ [zgnxanh]    ///  cd_common3 #h
@5928 = ~What do you want?~ [zgnxan0]    ///  cd_common4 #0
@5929 = ~I wish to adventure!~ [zgnxank_]   ///  cd_common5 #k_
@5932 = ~Hah-ha! Now that I like!~ [zgnxanz]    ///  cd_criticalhit #z
@5933 = ~Shar's blight on you, fool!~ [zgnxan1]    ///  cd_criticalmiss #1 (number)
@5934 = ~Ah!~ [zgnxanl]    ///  cd_damage #l (letter)
@5935 = ~Raah!~ [zgnxann_]   ///  cd_damage2 #n_
@5936 = ~Anh!~ [zgnxano_]   ///  cd_damage3 #o_
@5938 = ~What!?! How is that even possible?~ [zgnxan6]    ///  cd_disrupted #6
@5939 = ~There is much power to be found here, if we are watchful.~ [zgnxanp]    ///  cd_dungeon #p
@5940 = ~No! It cannot end like this!~ [zgnxanm]    ///  cd_dying #m
@5943 = ~You... sacrifice yourself for us?~ [zgnxanw]    ///  cd_gen_death #w
@5946 = ~I need healing, damn you!~ [zgnxane]    ///  cd_hurt #e
@5947 = ~Do something, damn you!~ [zgnxans_]   ///  cd_hurt2 #s_
@5948 = ~Well this clearly won't work.~ [zgnxan2]    ///  cd_immune #2
@5949 = ~I am not a mule! I refuse to carry everything!~ [zgnxan3]    ///  cd_inventory #3
@5950 = ~Everyone, together!~ [zgnxanb]    ///  cd_leader #b
@5951 = ~Protect me, would you? Thanks.~ [zgnxant_]   ///  cd_leader2 #t_
@5952 = ~You can't possibly win. I'm leaving.~ [zgnxanu_]   ///  cd_morale_break #u_
@5955 = ~Thanks. Really.~ [zgnxan4]    ///  cd_pickpocket #4
@5956 = ~If you are looking for idle 'chit-chat', look elsewhere.~ [zgnxanw_]   ///  cd_select_rare1 #w_
@5964 = ~Quiet it is.~ [zgnxan5]    ///  cd_shadows #5
@5965 = ~I think I need a little "me" time.~ [zgnxanc]    ///  cd_tired #c


